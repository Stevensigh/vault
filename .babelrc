{
  "env": {
    "production": {
      "plugins": [
        ["transform-react-remove-prop-types", {"removeImports": true}]
      ]
    }
  },
  "presets": [
    "env",
    "react",
    "stage-0"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "react-elemental",
    ["module-resolver", {
      "root": [
        "./src"
      ]
    }]
  ]
}
