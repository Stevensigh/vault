{
  "name": "react-static-boilerplate",
  "version": "0.1.2",
  "description": "Boilerplate for static websites in React",
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "webpack",
    "build:watch": "npm run build -- --watch",
    "start": "webpack-dev-server --watch",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "cover": "npm run test -- --coverage",
    "coveralls": "npm run cover && cat coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://git.kevinlin.info/personal/react-static-boilerplate.git"
  },
  "author": "Kevin Lin <developer@kevinlin.info>",
  "license": "MIT",
  "pre-commit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-elemental": "^1.1.2",
    "react-elemental-fonts": "^0.0.2",
    "react-helmet": "^5.2.0",
    "react-icons": "^2.2.7",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "supercharged": "^0.0.2"
  },
  "devDependencies": {
    "@linkiwi/eslint-config": "^1.1.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-react-elemental": "^1.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "coveralls": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.16.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-react": "^7.6.1",
    "html-loader": "^0.5.5",
    "html-webpack-inline-source-plugin": "^0.0.9",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.1.4",
    "nodemon": "^1.14.12",
    "pre-commit": "^1.2.2",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.2.0",
    "sinon": "^4.2.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  }
}
