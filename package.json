{
  "name": "vault",
  "version": "1.0.0",
  "description": "Secrets and credentials manager",
  "scripts": {
    "clean": "rm -rf dist/",
    "build:client": "webpack",
    "build:client:watch": "npm run build:client -- --watch",
    "build:server": "babel src/server -d dist/server",
    "build:server:watch": "npm run build:server -- --watch",
    "build:shared": "babel src/shared -d dist/shared",
    "build:shared:watch": "npm run build:shared -- --watch",
    "start:client": "webpack-dev-server --watch --disable-host-check",
    "start:server": "node -r dotenv/config -r babel-core/register src/server",
    "start:server:watch": "nodemon -r dotenv/config -r babel-core/register src/server",
    "dev": "concurrently --kill-others --prefix name --prefix-colors cyan.bold,yellow.bold --names Server,Client 'npm run start:server:watch' 'npm run build:client:watch'",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "cover": "npm run test -- --coverage",
    "coveralls": "npm run cover && cat coverage/lcov.info | coveralls",
    "postinstall": "npm run vault:generate-salt",
    "vault:run-script": "node -r dotenv/config -r babel-core/register -r babel-polyfill",
    "vault:generate-salt": "npm run vault:run-script -- src/server/scripts/generate-salt.js",
    "vault:set-master-password": "npm run vault:run-script -- src/server/scripts/set-master-password.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LINKIWI/vault.git"
  },
  "author": "Kevin Lin <developer@kevinlin.info>",
  "license": "MIT",
  "pre-commit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "cookie-parser": "^1.4.3",
    "copy-text-to-clipboard": "^1.0.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "fast-levenshtein": "^2.0.6",
    "humanize": "^0.0.9",
    "inquirer": "^5.2.0",
    "mysql": "^2.15.0",
    "prop-types": "^15.6.1",
    "raven": "^2.4.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-elemental": "^1.1.2",
    "react-elemental-fonts": "^0.0.2",
    "react-helmet": "^5.2.0",
    "react-icons": "^2.2.7",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "supercharged": "^0.0.3",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@linkiwi/eslint-config": "^1.1.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-react-elemental": "^1.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.5.1",
    "coveralls": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-react": "^7.7.0",
    "html-loader": "^0.5.5",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.4",
    "nodemon": "^1.17.2",
    "pre-commit": "^1.2.2",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.2.0",
    "sinon": "^5.0.7",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  }
}
